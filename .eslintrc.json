{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "spaceBeforeFunctionParen": true }
    ],
    "no-console": "warn",
    "func-names": "off",
    "space-before-function-paren": ["error", "always"]
  }
}
